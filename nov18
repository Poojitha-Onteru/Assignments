select * from customers where creditlimit>50000;
select * from employees where officeCode=1;
select * from orders where status='shipped' and orderDate like '2004%';
select * from products where buyprice between 50 and 100;
select * from employees where reportsTo='1143';
select * from customers where city like 'S%';
select * from orders where comments like '%urgent%';
select * from payments where paymentDate like '2003%';
select customerNumber,count(*) from payments group by customerNumber;
select customerNumber,count(*) from payments group by customerNumber having count(*)>3;
select officecode,count(*) from employees group by officecode;
select productline,avg(msrp) from products group by productLine having avg(msrp)>100;
select customernumber,sum(amount) from payments group by customerNumber having sum(amount)>50000;
select productcode,count(*) from products group by productCode;
select status,count(*) from orders group by status;
select reportsto,count(*) from employees group by reportsto having count(*)>2;
select * from customers order by creditLimit desc;
select * from products order by quantityInStock asc;
select * from payments order by paymentDate desc;
select * from employees order by jobTitle,lastname;
select creditlimit from customers order by creditLimit desc limit 5;
select productline from products order by productline asc; 
select * from orders order by orderDate desc limit 10;
select * from products order by productLine,buyPrice;
select * from orders o inner join customers m on o.customerNumber=m.customerNumber;
select e.employeenumber,m.employeenumber as manager from employees e left join employees m on e.employeeNumber=m.reportsTo;
#select p.productName,o.quantityordered from products p inner join orderdetails o on o.productCode=p.productCode;
SELECT p.productCode,p.productName,SUM(od.quantityOrdered) AS totalQuantity FROM products p inner JOIN orderdetails od ON p.productCode = od.productCode GROUP BY 
p.productCode, p.productName;
select c.customerNumber from customers c left join orders o on c.customerNumber=o.customerNumber;

