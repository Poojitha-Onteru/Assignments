#delete from orders where orderdate<'2005-01-01';
DELETE FROM orderdetails WHERE orderNumber IN (SELECT orderNumber FROM orders WHERE orderDate < '2005-01-01');
Delete from orders WHERE orderDate < '2005-01-01';
