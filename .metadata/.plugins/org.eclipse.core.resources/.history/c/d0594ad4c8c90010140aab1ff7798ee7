package com.generics;

public class ArrayPrinter<T> {
	private T arr;
	
	
	
	public ArrayPrinter() {
		super();
	}



	public ArrayPrinter(T arr) {
		super();
		this.arr = arr;
	}



	public T getArr() {
		return arr;
	}



	public void setArr(T arr) {
		this.arr = arr;
	}



	@Override
	public String toString() {
		return "ArrayPrinter [arr=" + arr + "]";
	}
	
	
	
}
