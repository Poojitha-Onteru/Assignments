package com.lambdapractice;

import java.util.function.Function;

public class DemoOnStaticMethodReference {


	static class NumberUtil{
		public static int squareNum(int num) {
			return num*num;
		}
		
		public static double log(double num) {
			return Math.log(num);
		}
	}
	
	public static void main(String[] args) {
		
		Function<Integer,Integer> squFuncLambda=(num)->NumberUtil.squareNum(num);
		Function<Integer,Integer> squareFuncMR=NumberUtil::squareNum;
		System.out.println(squFuncLambda.apply(8));
		System.out.println(squareFuncMR.apply(7));
		
		
		Function<Double,Double> logFuncMR=NumberUtil::log;
		Function<Double,Double> logFuncLambda=(num)->NumberUtil.log(num);
		
		System.out.println(logFuncMR.apply(8.0));
		System.out.println(logFuncLambda.apply(6.0));
		
	}
	
	

}
