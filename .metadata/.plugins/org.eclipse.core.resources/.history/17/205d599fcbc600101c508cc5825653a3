package com.practice1;

public class Calculator implements ScientificCalculatorInterface {

	@Override
	public double power(double m, double n) {
		return Math.pow(m,n);
	}

	@Override
	public double squareRoot(double m) {
		return Math.sqrt(m);
	}

	@Override
	public double lo(double m) {
		return Math.log(m);
	}

}
