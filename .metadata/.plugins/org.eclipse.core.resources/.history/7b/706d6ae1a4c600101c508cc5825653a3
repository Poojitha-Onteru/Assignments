package com.practice1;

public class Account {
	
	private long accNum;
	private String accHolder;
	private String ifsc;
	private String branch;
	private String bankName;
	private int pin;
	public Account() {
		super();
	}
	public Account(long accNum, String accHolder, String ifsc, String branch, String bankName) {
		super();
		this.accNum = accNum;
		this.accHolder = accHolder;
		this.ifsc = ifsc;
		this.branch = branch;
		this.bankName = bankName;
		this.pin=generatePin();
	}
	
	private int generatePin() {
		int newPin=(int) (Math.random()*10000);
		return newPin;
	}
	
	@Override
	public String toString() {
		return "Account [accNum=" + accNum + ", accHolder=" + accHolder + ", ifsc=" + ifsc + ", branch=" + branch
				+ ", bankName=" + bankName + ", pin=" + pin + "]";
	}
	
	

}
