package practice2.StringClasses;

public class ArrayPractice {
	public static void main(String ard[]) {
		Employee emp[]=new Employee[5];
		
		Employee e1=new Employee(1,"pooji","pooji@empil.com");
		Employee e2=new Employee(2,"riya","riya123@gmail.com");
		Employee e3=new Employee(3,"priya","priya2003@gmail.com");
		Employee e4=new Employee(4,"sai","sai@gmail.com");
		Employee e5=new Employee(5,"sreya","sreya@gmail.com");
		
		emp[0]=e1;
		emp[1]=e2;
		emp[2]=e3;
		emp[3]=e4;
		emp[4]=e5;
		
		System.out.println(emp[0].changeEmail(e1, "pooji123@gmail.com"));
		
		
		
	}

}
